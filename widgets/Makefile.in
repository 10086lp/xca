VERSION=@PACKAGE_VERSION@
TARGET=xca-$(VERSION)

GCC=@CXX@
CC=@CC@

MOC=@MOC@
UIC=@UIC@

AR=@AR@
RANLIB=@RANLIB@

CPPFLAGS=-I.. @CPPFLAGS@ @DEFS@
CFLAGS=@ac_CC_WARN@ @ac_DEBUG@

##############################################

NAMES= ExportCert ExportKey ExportTinyCA NewX509 CrlDetail CertDetail \
	MainWindow validity distname ReqDetail

OBJS=$(patsubst %,moc_%.o,$(NAMES)) $(patsubst %,%.o,$(NAMES))

MOC_CPP=$(patsubst %,moc_%.cpp,$(NAMES))

all: widgets.o

widgets.o: $(OBJS) 
	$(LD) -r -o $@ $(OBJS)

%.o: %.cpp 
	$(CC) $(CPPFLAGS) $(CFLAGS) -c $< -o $@
clean:
	rm -rf *~ *.o *.a $(MOC_CPP)  

distclean: clean
	rm -f Makefile 

moc_%.cpp: %.h %.cpp
	$(MOC) $< -o $@

.SECONDARY: $(UI_CPP) $(UI_H) $(MOC_CPP)  
